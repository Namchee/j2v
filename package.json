    "@vitest/coverage-v8": "^2.1.1",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@namchee/parsley": "^1.0.1",
    "cac": "^6.7.14",
    "detect-package-manager": "^3.0.2",
    "ora": "^8.1.0",
